:global AddressList
:global ForwardTo
/ip dns static
:if ([:len [/ip dns static find name="argotunnel.com" type=FWD match-subdomain=yes]] = 0) do={ add address-list=\$AddressList forward-to=\$ForwardTo comment="cloudflare" match-subdomain=yes type=FWD name="argotunnel.com" }
