:global AddressList
:global ForwardTo
/ip dns static
:if ([:len [/ip dns static find name="netflix.com.edgesuite.net" comment="netflix"]] = 0) do={ add address-list=$AddressList forward-to=$ForwardTo comment="netflix" type=FWD name="netflix.com.edgesuite.net" }
:if ([:len [/ip dns static find name="fast.com" comment="netflix"]] = 0) do={ add address-list=$AddressList forward-to=$ForwardTo comment="netflix" match-subdomain=yes type=FWD name="fast.com" }
:if ([:len [/ip dns static find name="netflix.com" comment="netflix"]] = 0) do={ add address-list=$AddressList forward-to=$ForwardTo comment="netflix" match-subdomain=yes type=FWD name="netflix.com" }
